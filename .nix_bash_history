sudo mv /home/$USER/Downloads/configuration.nix /etc/nixos/configuration.nix
sudo nano /etc/nixos/configuration.nix
sudo nixos-rebuild switch --upgrade && sudo nix-store --optimise
sudo nixos-rebuild switch
